					    <script type="text/javascript">
							
							// center
							var map = new ol.Map({
							target: 'map',
							layers: [
							  new ol.layer.Tile({
							    source: new ol.source.OSM()
							  })
							],
							view: new ol.View({
							  center: ol.proj.fromLonLat([39.718556, 41.000321]),
							  zoom: 5
							})
							});
							
			
							var layer = new ol.layer.Vector({
							 source: new ol.source.Vector({
							     features: [

							     // trabzon
							     new ol.Feature({
							             geometry: new ol.geom.Point(ol.proj.fromLonLat([39.718556, 41.000321]))
							         }),

							     // rize
							     new ol.Feature({
							             geometry: new ol.geom.Point(ol.proj.fromLonLat([40.511075, 41.029905]))
							         }),

							     // artvin
							     new ol.Feature({
							             geometry: new ol.geom.Point(ol.proj.fromLonLat([41.820998, 41.180434]))
							         }),

							     // gümüşhane 
							     new ol.Feature({
							             geometry: new ol.geom.Point(ol.proj.fromLonLat([39.480057,40.461759]))
							         }),

							     // giresun
